@model IEnumerable<AppX.Models.Departmentx>

@{
    ViewBag.Title = "Departments";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Dep_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dep_location)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Dep_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dep_location)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Dep_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Dep_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Dep_id })
        </td>
    </tr>
}

</table>
